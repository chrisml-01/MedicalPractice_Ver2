      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MedicalPractice_Ver2.View"
      xmlns:mycontrols="clr-namespace:MedicalPractice_Ver2.UserControls"
      mc:Ignorable="d" 
      d:DesignHeight="630" d:DesignWidth="1000" Background="White"
      Title="Users">

    <Grid>
        <TabControl HorizontalAlignment="Left" Height="610" VerticalAlignment="Top" Width="980" Margin="10,10,0,0">
            <TabItem Header="Patient" Height="30" Width="80">
                <Frame Name="Patient" NavigationUIVisibility="Hidden"/>
            </TabItem>
            <TabItem Header="Staff" Height="30" Width="80">
                <Frame Name="Staff" NavigationUIVisibility="Hidden"/>
            </TabItem>
            <TabItem Header="Practitioner" Width="80">
                <Frame Name="Practitioner" NavigationUIVisibility="Hidden"/>
            </TabItem>
        </TabControl>

    </Grid>
</Page>
